      {
        "tab": "Valley Correctional Facility",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "home",
              "information",
              "notices"
            ]
          },
          {
            "group": "About Us",
            "pages": [
              "about-us/our-team",
              "about-us/moderation-division"
            ]
          },
          {
            "group": "Community Rules",
            "pages": [
              "community-rules/our-rules"
            ]
          },
          {
            "group": "Shift Information",
            "pages": [
              "shift-information/shift-information",
              "shift-information/roleplay-information"
            ]
          },
          {
            "group": "Miscellaneous",
            "pages": [
              "miscellaneous/updates"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Discord",
          "href": "https://discord.gg/vprp",
          "icon": "discord"
