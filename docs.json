{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "linden",
  "name": "Valley Correctional Facility",
  "colors": {
    "primary": "#a8a8a8",
    "light": "#ffa200",
    "dark": "#ffa200"
  },
  "errors": {
  "404": {
    "redirect": false,
    "title": "Something went wrong",
    "description": "We couldn't find what you where looking for. Trying check the URL to ensure it's correct. Otherwise, the page just might not exist."
  }
},
  "banner": {
  "content": "Welcome to the new VCF documentation site! Here you can easily find information about the community and our rules.",
  "dismissible": true 
},
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guidelines & Information",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "introduction"
            ]
          },
          {
            "group": "About Us",
            "pages": [
              "about-us/our-team",
              "about-us/moderation-team"
            ]
          },
          {
            "group": "Shift Rules & Information",
            "pages": [
              "shift-rules-information/shift-information",
              "shift-rules-information/shift-rules",
              "shift-rules-information/whitelist",
              "shift-rules-information/roleplay-information"
            ]
          },
          {
            "group": "Discord Server Rules",
            "pages": [
              "discord-server-rules/chat-rules",
              "discord-server-rules/voice-rules"
            ]
          },
          {
            "group": "Miscellaneous",
            "pages": [
              "miscellaneous/updates"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Discord",
          "href": "https://discord.gg/vprp",
          "icon": "discord"
        }
      ]
    }
  },
  "contextual": {
    "options": [
{
                "title": "Leave feedback",
                "description": "Leave your feedback or report an issue so we can fix it!",
                "icon": "thought-bubble",
                "href": "https://forms.gle/pzkiU5ujJ1zMpm9d6"
            }
    ]
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "footer": {
    "socials": {
      "discord": "https://discord.gg/vprp"
    }
  }
  
  
}