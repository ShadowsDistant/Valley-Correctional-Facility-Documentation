{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "linden",
  "name": "Valley Correctional Facility",
  "thumbnails": {
    "appearance": "dark"
  },
  "colors": {
    "primary": "#a8a8a8",
    "light": "#ffa200",
    "dark": "#ffa200"
  },
  "errors": {
    "404": {
      "redirect": false,
